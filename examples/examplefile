(html
    (doctype "html5")
    (head
        (title
	    (text "The title of this example roundy document")))
    (body
        (h1
            (class "headline")
            (text "the header"))
        (img
            (src "images/icon.gif")
            (alt "an icon which you can't see!"))
        (p
            (text "Here is some text with < and > and />."))
        (p
            (id "foo"))
	(div
	    (div
	        (div
		    (div
		        (div)))))))
